        orientation: "vertical"
        canvas.before:
            Rectangle:
                id: png
                source: "ASSETS/back/Background.png"
                size: self.size
                pos: self.pos
        Image:
            id: png
            source: "ASSETS/LogoW.png"
            size_hint: 1, .3
