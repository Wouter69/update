            font_name: "ASSETS/font/CelticGaramondthe2nd.ttf"
            font_size: 18
            size_hint: 1, None
            width:
            height: dp(30)
        Label:
            id: WelcomeName
            text: 'Name of Player'
            text_size: None,None
