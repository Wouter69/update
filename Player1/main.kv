    title_color: 0,0,0,1
    separator_height: dp(0)
    background: 'ASSETS/back/PopupB.jpg'

    BoxLayout:
        orientation: 'vertical'
        spacing: dp(5)
        Label:
            text: 'Welcome'
            text_size: None, None
            valign: 'center'
