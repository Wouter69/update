            font_name: "ASSETS/font/CelticGaramondthe2nd.ttf"
            font_size: 16
            size_hint: 1, None
            width:
            height: dp(30)
        Label:
            text: 'You currently rank at:'
            text_size: None,None
            valign: 'center'
            color: 0,0,0,1
            font_name: "ASSETS/font/CelticGaramondthe2nd.ttf"
            font_size: 16
            size_hint: 1, None
            width:
            height: dp(30)
        Label:
            id: Rank
            text: 'Rank'
            text_size: None,None
            valign: 'center'
            color: 0,0,0,1
            font_name: "ASSETS/font/CelticGaramondthe2nd.ttf"
            font_size: 25
            size_hint: 1, None
            width:
            height: dp(70)
        Button:
            id: continue_button
            size_hint: None, None
            width: dp(160)
            height: dp(40)
            pos_hint:{'center_x': .5, }
            background_color: 0,0,0,0

            on_press: root.continue_on()
            on_release: root.continue_off(), root.dismiss(), Factory.Home()

            Image:
                id: continue_image
                source:'ASSETS/Buttons/OnContinue.png'
                center_x: self.parent.center_x
                center_y: self.parent.center_y
