<Home>:
    BoxLayout:
        spacing: dp(40)
        padding:dp(10)
        orientation: "vertical"
        canvas.before:
            Rectangle:
                id: png
                source: "ASSETS/back/Background.png"
                size: self.size
                pos: self.pos

        BoxLayout:
            size_hint: 1, None
            width:
            height: dp(120)
            #pos_hint: {'center_x': .5, 'Top': .0}
            Image:
                id: Icon_
                source: "ASSETS/PlayerIcon/SagittariusM.png"
                size_hint: None, None
                width: dp(120)
                height: dp(120)
            Label:
                id: Player
                text: 'Pampoen Pit'
                #text_size: self.width,None
                color: 1,1,1,1
                bold: True
                outline_color: 0,0,0,1
                outline_width: 3
                font_name: "ASSETS/font/CelticGaramondthe2nd.ttf"
                font_size: 25
                size_hint: self.width,None
                width:
                height: dp(40)
        Button:
            text: "Store"
            color: 0,0,0,1
            font_name: "ASSETS/font/CelticGaramondthe2nd.ttf"
            font_size: 20
            color: 190/255,147/255,97/255,1
            size_hint: .5, None
            width:
            height: dp(80)
            pos_hint: {'center_x': .5}
            background_color: 79/255,41/255,9/255,1
            on_press: root.ads()
        Button:
            text: "Play"
            color: 0,0,0,1
            font_name: "ASSETS/font/CelticGaramondthe2nd.ttf"
            font_size: 20
            color: 190/255,147/255,97/255,1
            size_hint: .5, None
            width:
            height: dp(80)
            pos_hint: {'center_x': .5}
            background_color: 79/255,41/255,9/255,1
            on_press: root.move()
        Button:
            text: "Rank"
            color: 0,0,0,1
            font_name: "ASSETS/font/CelticGaramondthe2nd.ttf"
            font_size: 20
            color: 190/255,147/255,97/255,1
            size_hint: .5, None
            width:
            height: dp(80)
            pos_hint: {'center_x': .5}
            background_color: 79/255,41/255,9/255,1
        Button:
            text: "Settings"
            color: 0,0,0,1
            font_name: "ASSETS/font/CelticGaramondthe2nd.ttf"
            font_size: 20
            color: 190/255,147/255,97/255,1
            size_hint: .5, None
            width:
            height: dp(40)
            background_color: 79/255,41/255,9/255,1

<Welcome>:
    auto_dismiss: False
    size_hint: .9, .5
    pos_hint:{'center_x': .5, 'center_y': .5}
