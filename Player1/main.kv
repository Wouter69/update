<Game>:
    MDFloatLayout:
        
        Image:
            pos_hint: {'x': 0.0,'y': -0.2}
            size_hint: 1.5, 1.5
            source: "ASSETS/back/GameB.jpg"
            allow_stretch: True

        Button:
            id: LOBBY_G
            background_normal: "ASSETS/back/GameB.jpg"
            text: 'LOBBY_G'
            size_hint: .4, .05
            pos_hint: {'x': 0, 'y': .95}

        Button:
            id: POINTS
            background_normal: "ASSETS/back/GameB.jpg"
            text: 'POINTS'
            size_hint: .4, .05
            pos_hint: {'x': .4, 'y': .95}

        Button:
            id: QUIT
            text: "QUIT"
            size_hint: .2, .05
            pos_hint: {'x': .8, 'y': .95}
            on_press: root.home_it()

        Button:
            id: Hit_Me
            text:'Hit_Me'
            background_normal: "ASSETS/FTCard/TreeJ.png"
            size_hint: .2, .2
            pos: 230, 250
            on_press: root.at_hand()

        Button:
            id: Hit_Me
            text:'Hit_Me'
            background_normal: "ASSETS/FTCard/TreeJ.png"
            size_hint: .2, .2
            pos: 10, 250
            on_press: root.at_hand()

        Button:
            id: oppTable
            text: 'OPP_TABLE'
            size_hint: .95, .2
            pos_hint: {'x': 0.025,'y': 0.64}

        Button:
            id: myTable
            text: 'MY_TABLE'
            size_hint: .95, .2
            pos_hint: {'x': 0.025,'y': 0.25}

        Button:
            id: oppIcon
            text: "ICON"
            size_hint: .2, .12
            pos_hint: {'x': 0.4,'y': 0.83}
        Button:
            id: oppName
            text: "OppName"
            size_hint: .45, .02
            pos_hint: {'x': 0.28,'y': 0.82}

        Button:
            id: myIcon
            text: "ICON"
            size_hint: .2, .12
            pos_hint: {'x': 0.4,'y': 0.45}
        Button:
            id: myName
            text: "ICON"
            size_hint: .45, .02
            pos_hint: {'x': 0.28,'y': 0.45}

        TABS:
            size_hint: 1, .25
            pos_hint: {'x': 0.0,'y': 0.0}

